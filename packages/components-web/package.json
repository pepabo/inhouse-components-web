{
  "name": "@pepabo-inhouse/components-web",
  "description": "Inhouse UI components for the web",
  "version": "2.0.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/pepabo/inhouse-components-web.git",
    "directory": "packages/inhouse-components-web"
  },
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@pepabo-inhouse/adapter": "^2.0.0",
    "@pepabo-inhouse/app-bar": "^2.0.0",
    "@pepabo-inhouse/avatar": "^2.0.0",
    "@pepabo-inhouse/bottom-navigation": "^2.0.0",
    "@pepabo-inhouse/button": "^2.0.0",
    "@pepabo-inhouse/callout": "^2.0.0",
    "@pepabo-inhouse/card": "^2.0.0",
    "@pepabo-inhouse/cell": "^2.0.0",
    "@pepabo-inhouse/checkbox": "^2.0.0",
    "@pepabo-inhouse/constants": "^2.0.0",
    "@pepabo-inhouse/container": "^2.0.0",
    "@pepabo-inhouse/description-list": "^2.0.0",
    "@pepabo-inhouse/flavor": "^2.0.0",
    "@pepabo-inhouse/grid": "^2.0.0",
    "@pepabo-inhouse/header": "^2.0.0",
    "@pepabo-inhouse/icon": "^2.0.0",
    "@pepabo-inhouse/interactive-list": "^2.0.0",
    "@pepabo-inhouse/interactive-table": "^2.0.0",
    "@pepabo-inhouse/list": "^2.0.0",
    "@pepabo-inhouse/navigation-drawer": "^2.0.0",
    "@pepabo-inhouse/progress-indicator": "^2.0.0",
    "@pepabo-inhouse/radio": "^2.0.0",
    "@pepabo-inhouse/scrim": "^2.0.0",
    "@pepabo-inhouse/select": "^2.0.0",
    "@pepabo-inhouse/side-navigation": "^2.0.0",
    "@pepabo-inhouse/skeleton": "^2.0.0",
    "@pepabo-inhouse/snackbar": "^2.0.0",
    "@pepabo-inhouse/table": "^2.0.0",
    "@pepabo-inhouse/textfield": "^2.0.0",
    "@pepabo-inhouse/thumbnail": "^2.0.0",
    "@pepabo-inhouse/tokens": "^0.17.0",
    "autoprefixer": "10.4.14",
    "cssnano": "5.1.15",
    "postcss": "^8.4.38",
    "postcss-cli": "10.1.0",
    "sass": "1.62.1"
  },
  "scripts": {
    "build": "npm run clean && npm run compile",
    "clean": "rm -rf dist",
    "compile": "mkdir dist && npm run compile:scss && npm run compile:postcss:min && npm run compile:postcss:unmin",
    "compile:scss": "sass inhouse-components-web.scss:dist/inhouse-components-web.css -I ../../node_modules --no-source-map",
    "compile:postcss:unmin": "postcss dist/inhouse-components-web.css --output dist/inhouse-components-web.css --use autoprefixer",
    "compile:postcss:min": "postcss dist/inhouse-components-web.css --output dist/inhouse-components-web.min.css --use autoprefixer --use cssnano",
    "watch": "npm run compile:scss -- --watch"
  },
  "browserslist": [
    "last 2 versions"
  ]
}
